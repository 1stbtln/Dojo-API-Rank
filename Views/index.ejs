<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>Dojo Ranking</title>
    <link rel="stylesheet" href="/style.css">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Bungee+Spice&display=swap" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
</head>
<body class="css-selector">
    <div id="ekwfl">
        <h1>Dojo <br> LeaderBoard</h1>
        <p id="p1">Dojo Sensei, Master Stat's LeaderBoard</p>
    </div>
    <div class="div2">

      <video autoplay muted loop id="video-background">
        <source src="/VS Backgroun.mp4" type="video/mp4">
        Your browser does not support HTML5 video.
    </video>

    <% players.forEach(function(player) { %>
      <div class="dropdown x11 css-selector">
        <button class="btn btn-secondary dropdown-toggle" type="button" data-bs-toggle="dropdown" aria-expanded="false">
          <%= player.name %>
        </button>
        <ul class="dropdown-menu">
          <li><a class="dropdown-item" href="#"><b>Account ID:</b> <i><%= player.accountId %></i></a></li>
          <li><a class="dropdown-item" href="#"><b>Rounds Played:</b> <i><%= player.roundsPlayed %></i></a></li>
          <li><a class="dropdown-item" href="#"><b>Total Damage:</b> <i><%= player.overallDamage %></i></a></li>
          <li><a class="dropdown-item" href="#"><b>Total Kills:</b> <i><%= player.totalKills %></i></a></li>
          <% // Include additional stats here if needed %>
        </ul>
      </div>
    <% }); %>
    
          
        

          

          
        
    </div>

    <div class="div3">
        <h2>Top Numbers</h2>
        <div class="div3Div1">
            <h3> Impereal</h3>
            <ul>"damageDealt": 614,126</ul>
            <ul>"kills": 3571</ul>
            <ul>"dBNOs": 3594</ul>
            <ul>"wins": 152</ul>
        </div>
        <div class="div3Div2">
            <h3> BajaMike</h3>
            <ul>"damageDealt": 414,126</ul>
            <ul>"kills": 1463</ul>
            <ul>"dBNOs": 2381</ul>
            <ul>"wins": 345</ul>
        </div>

    </div>
    
    

</body>
</html>
